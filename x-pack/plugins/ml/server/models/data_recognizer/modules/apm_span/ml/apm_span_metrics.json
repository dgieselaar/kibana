{
  "job_type": "anomaly_detector",
  "groups": [
    "apm"
  ],
  "description": "Detects anomalies in span latency, throughput and error percentage for metric data.",
  "analysis_config": {
    "bucket_span": "15m",
    "summary_count_field_name" : "doc_count",
    "detectors" : [
      {
        "detector_description" : "high latency by exit span destination for an APM service",
        "function" : "high_mean",
        "field_name" : "span_latency",
        "by_field_name" : "span.destination.service.resource",
        "partition_field_name" : "service.name"
      },
      {
        "detector_description" : "exit span throughput for an APM service",
        "function" : "mean",
        "field_name" : "total_count",
        "by_field_name" : "span.destination.service.resource",
        "partition_field_name" : "service.name"
      },
      {
        "detector_description" : "failed exit span rate for an APM service",
        "function" : "high_mean",
        "field_name" : "failed_span_rate",
        "by_field_name" : "span.destination.service.resource",
        "partition_field_name" : "service.name"
      }
    ],
    "influencers" : [
      "span.destination.service.resource",
      "service.name"
    ]
  },
  "analysis_limits": {
    "model_memory_limit": "32mb"
  },
  "data_description": {
    "time_field" : "@timestamp",
    "time_format" : "epoch_ms"
  },
  "model_plot_config": {
    "enabled" : true,
    "annotations_enabled" : true
  },
  "results_index_name" : "custom-apm",
  "custom_settings": {
    "created_by": "ml-module-apm-span",
    "managed": true
  }
}
