{
  "id": "apm_span",
  "title": "APM",
  "description": "Detect anomalies in exit span latency, throughput and failure rate from your APM services for metric data.",
  "type": "Span data",
  "logoFile": "logo.json",
  "defaultIndexPattern": "apm-*-metric,metrics-apm*",
  "query": {
    "bool": {
      "filter": [
        { "term": { "processor.event": "metric" } },
        { "term": { "metricset.name": "service_destination" } }
      ],
      "must_not": { "terms": { "_tier": [ "data_frozen", "data_cold" ] } }
    }
  },
  "jobs": [
    {
      "id": "apm_span_metrics",
      "file": "apm_span_metrics.json"
    }
  ],
  "datafeeds": [
    {
      "id": "datafeed-apm_span_metrics",
      "file": "datafeed_apm_span_metrics.json",
      "job_id": "apm_span_metrics"
    }
  ]
}
